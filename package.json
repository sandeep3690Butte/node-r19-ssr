{
  "name": "node-ssr-react19",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "repository": "https://github.com/sandeep3690Butte/node-r19-ssr.git",
  "author": "sandeep Butte <sandeep.butte@nobroker.in>",
  "scripts": {
    "dev": "NODE_ENV=development webpack --config webpack.client.dev.js --watch & webpack --config webpack.server.js --watch & nodemon --watch dist/server --exec \"node dist/server/main.js\"",
    "build:client": "webpack --config webpack.client.prod.js",
    "build:server": "webpack --config webpack.server.js",
    "build": "yarn clean && npm run build:client && npm run build:server",
    "start": "NODE_ENV=production node dist/server/main.js",
    "clean": "rm -rf dist loadable-stats"
  },
  "dependencies": {
    "core-js": "3",
    "express": "^5.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "6.30.2",
    "react-router-dom": "6.30.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@loadable/babel-plugin": "^5.16.1",
    "@loadable/component": "^5.16.7",
    "@loadable/server": "^5.16.7",
    "@loadable/webpack-plugin": "^5.15.2",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.5",
    "mini-css-extract-plugin": "^2.9.4",
    "source-map-support": "^0.5.21",
    "style-loader": "^4.0.0",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
